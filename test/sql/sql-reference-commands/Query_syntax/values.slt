query TT
SELECT * FROM (VALUES (1, 'one'), (2, 'two'), (3, 'three'))
----
1	one
2	two
3	three

query TT
SELECT column1, $2 FROM (VALUES (1, 'one'), (2, 'two'), (3, 'three'))
----
1	one
2	two
3	three

query TT
SELECT v1.$2, v2.$2
  FROM (VALUES (1, 'one'), (2, 'two')) AS v1
        INNER JOIN (VALUES (1, 'One'), (3, 'three')) AS v2
  WHERE v2.$1 = v1.$1
----
one	One

query TT
SELECT c1, c2
  FROM (VALUES (1, 'one'), (2, 'two')) AS v1 (c1, c2)
----
1	one
2	two

