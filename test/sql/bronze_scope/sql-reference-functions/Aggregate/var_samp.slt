WITH aggr(k, v, v2) AS (
    SELECT $1, $2, $3 FROM VALUES
        (1, 10, NULL),
        (2, 10, 11),
        (2, 20, 22),
        (2, 25, NULL),
        (2, 30, 35)
)
SELECT k, var_samp(v), var_samp(v2)
    FROM aggr
    GROUP BY k
    ORDER BY k;
