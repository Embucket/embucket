query T
WITH sample_table(k, d) AS (
    SELECT $1, $2 FROM VALUES
    ('1', '1'), ('1', '5'), ('1', '3'),
    ('2', '2'), ('2', NULL),
    ('3', NULL),
    (NULL, '7'), (NULL, '1')
)
SELECT MAX(d) FROM sample_table
----
7

