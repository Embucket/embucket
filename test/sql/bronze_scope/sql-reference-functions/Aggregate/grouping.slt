query TTTTTT
WITH aggr2(col_x, col_y, col_z) AS (
    SELECT $1, $2, $3 FROM VALUES
        (1, 2, 1),
        (1, 2, 3),
        (2, 1, 10),
        (2, 2, 11),
        (2, 2, 3)
)
SELECT col_x, col_y, sum(col_z),
       grouping(col_x), grouping(col_y), grouping(col_x, col_y)
    FROM aggr2 GROUP BY GROUPING SETS ((col_x), (col_y), ())
    ORDER BY 1, 2;
