WITH sum_example(k, d, s1, s2) AS (
  SELECT $1, $2, $3, $4 FROM VALUES
    (1, 1.1, '1.1', 'one'),
    (1, 10, '10', 'ten'),
    (2, 2.2, '2.2', 'two'),
    (2, NULL, NULL, 'null'),
    (3, NULL, NULL, 'null'),
    (NULL, 9, '9.9', 'nine')
)
SELECT
  SUM(d),
  SUM(CAST(s1 AS DECIMAL(10, 5)))
FROM sum_example;
