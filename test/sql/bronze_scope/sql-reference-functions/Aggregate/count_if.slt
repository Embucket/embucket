query T
WITH basic_example(i_col, j_col) AS (
    SELECT $1, $2 FROM VALUES
        (11, 101), (11, 102), (11, NULL), (12, 101), (NULL, 101), (NULL, 102)
)
SELECT COUNT_IF(TRUE) FROM basic_example;
