CREATE OR REPLACE TABLE rlike_ex(city VARCHAR(20));

exclude-from-coverage
statement ok
INSERT INTO rlike_ex VALUES ('Sacramento'), ('San Francisco'), ('San Jose'), (null);

query T
SELECT * FROM rlike_ex WHERE RLIKE(city, 'san.*', 'i')
