'2016-01-01T01:00:59'	'2016-01-01T02:00:00'	1	60	3541

query TT
SELECT d AS "Date",
       DATEDIFF('week', '2017-01-01'::DATE, d) AS "Weeks Diff from 2017-01-01 to Date"
  FROM VALUES
       ('2016-12-30'::DATE),
       ('2016-12-31'::DATE),
       ('2017-01-01'::DATE),
       ('2017-01-02'::DATE),
       ('2017-01-03'::DATE),
       ('2017-01-04'::DATE),
       ('2017-01-05'::DATE),
       ('2017-12-30'::DATE),
       ('2017-12-31'::DATE)
  AS t(d);
----
'2016-12-30'	0
'2016-12-31'	0
'2017-01-01'	0
'2017-01-02'	1
'2017-01-03'	1
'2017-01-04'	1
'2017-01-05'	1
'2017-12-30'	52
'2017-12-31'	52
