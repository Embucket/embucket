
query TTT
WITH task_history_test_table AS (
  SELECT 1 AS id
),
TASK_HISTORY AS (
  SELECT 'TASK_HISTORY_TEST_TASK' AS NAME, 'SCHEDULED' AS STATE, 'INSERT INTO task_history_test_table VALUES (1)' AS QUERY_TEXT
)
SELECT
  NAME,
  STATE,
  QUERY_TEXT
FROM TASK_HISTORY
WHERE NAME = 'TASK_HISTORY_TEST_TASK'
LIMIT 1;
----
TASK_HISTORY_TEST_TASK	SCHEDULED	INSERT INTO task_history_test_table VALUES (1)
