query TT
WITH vardttm AS (
  SELECT TO_VARIANT(TO_DATE('2024-02-24')) AS v UNION ALL
  SELECT TO_VARIANT(TO_TIME('20:57:01.123456789+07:00')) UNION ALL
  SELECT TO_VARIANT(TO_TIMESTAMP('2023-02-24 12:00:00.456')) UNION ALL
  SELECT TO_VARIANT(TO_TIMESTAMP_LTZ('2022-02-24 13:00:00.123 +01:00')) UNION ALL
  SELECT TO_VARIANT(TO_TIMESTAMP_NTZ('2021-02-24 14:00:00.123 +01:00')) UNION ALL
  SELECT TO_VARIANT(TO_TIMESTAMP_TZ('2020-02-24 15:00:00.123 +01:00'))
)
SELECT v, TYPEOF(v) AS type FROM vardttm
----
"2024-02-24"	DATE
"20:57:01"	TIME
"2023-02-24 12:00:00.456"	TIMESTAMP_NTZ
"2022-02-24 04:00:00.123 -0800"	TIMESTAMP_LTZ
"2021-02-24 14:00:00.123"	TIMESTAMP_NTZ
"2020-02-24 15:00:00.123 +0100"	TIMESTAMP_TZ

